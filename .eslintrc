{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "max-len": [
      2,
      {
        "code": 120,
        "ignoreComments": true
      }
    ],
    "arrow-body-style": 0,
    "quote-props": 0,
    "eol-last": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "react/jsx-closing-bracket-location": [
      2,
      "after-props"
    ],
    "react/prop-types": [
      2,
      {
        "ignore": []
      }
    ],
    "react/no-multi-comp": [
      0
    ],
    "react/prefer-stateless-function": 0,
    "react/jsx-curly-spacing": 0
  }
}
